{
  "name": "Tss tss tss",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Reveal hidden content",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.leparisien.fr/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "base.js",
        "./websites/leparisien.js"
      ]
    },
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "base.js",
        "./websites/instagram.js"
      ]
    },
    {
      "matches": [
        "https://www.liberation.fr/*"
      ],
      "js": [
        "base.js",
        "./websites/liberation.js"
      ]
    }
  ]
}